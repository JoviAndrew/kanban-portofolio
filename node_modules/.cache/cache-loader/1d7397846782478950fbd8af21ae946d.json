/div>\n      <div class=\"col-lg-3 col-sm-12\">\n        <div class=\"card border-danger mb-3\" style=\"max-width: 20rem;\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title\">Doing</h4>\n          </div>\n          <div class=\"card-body\">\n            <div class=\"jumbotron\" v-for=\"(task, index) in doingTasks\" :key=\"index\">\n              <h4>{{task.taskName}}</h4>\n              <hr class=\"my-4\">\n              <p>{{task.taskDes}}</p>\n              <p>{{task.person}}</p>\n              <p>{{task.taskPts}}</p>\n              <p class=\"lead\">\n                <button class=\"btn btn-secondary\" v-on:click=\"backToBacklog(index)\">prev</button>\n                <button class=\"btn btn-info\" v-on:click=\"addWaiting(index)\">next</button>\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-sm-12\">\n        <div class=\"card border-warning mb-3\" style=\"max-width: 20rem;\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title\">Waiting</h4>\n          </div>\n          <div class=\"card-body\">\n            <div class=\"jumbotron\" v-for=\"(task, index) in waitingTasks\" :key=\"index\">\n              <h4>{{task.taskName}}</h4>\n              <hr class=\"my-4\">\n              <p>{{task.taskDes}}</p>\n              <p>{{task.person}}</p>\n              <p>{{task.taskPts}}</p>\n              <p class=\"lead\">\n                <button class=\"btn btn-secondary\" v-on:click=\"backToDoing(index)\">prev</button>\n                <button class=\"btn btn-info\" v-on:click=\"addDone(index)\">next</button>\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-lg-3 col-sm-12\">\n        <div class=\"card border-success mb-3\" style=\"max-width: 20rem;\">\n          <div class=\"card-header\">\n            <h4 class=\"card-title\">Done</h4>\n          </div>\n          <div class=\"card-body\">\n            <div class=\"jumbotron\" v-for=\"(task, index) in doneTasks\" :key=\"index\">\n              <h4>{{task.taskName}}</h4>\n              <hr class=\"my-4\">\n              <p>{{task.taskDes}}</p>\n              <p>{{task.person}}</p>\n              <p>{{task.taskPts}}</p>\n              <p class=\"lead\">\n                <button class=\"btn btn-secondary\" v-on:click=\"backToWaiting(index)\">prev</button>\n                <button class=\"btn btn-info\" v-on:click=\"finishTask(index)\">next</button>\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: 'KanbanCards',\n  data: function () {\n    return {\n      doingTasks: [],\n      waitingTasks: [],\n      doneTasks: []\n    }\n  },\n  props: ['backlogTasks'],\n  methods: {\n    addDoing: function (index) {\n      this.doingTasks.push(this.backlogTasks[index])\n      this.backlogTasks.splice(index, 1)\n    },\n    addWaiting: function (index) {\n      this.waitingTasks.push(this.doingTasks[index])\n      this.doingTasks.splice(index, 1)\n    },\n    addDone: function (index) {\n      this.doneTasks.push(this.waitingTasks[index])\n      this.waitingTasks.splice(index, 1)\n    },\n    finish: function (index) {\n      this.doneTasks.splice(index, 1)\n    },\n    backToBacklog: function (index) {\n      this.backlogTasks.push(this.doingTasks[index])\n      this.doingTasks.splice(index, 1)\n    },\n    backToDoing: function (index) {\n      this.doingTasks.push(this.waitingTasks[index])\n      this.waitingTasks.splice(index, 1)\n    },\n    backToWaiting: function (index) {\n      this.waitingTasks.push(this.doneTasks[index])\n      this.doneTasks.splice(index, 1)\n    }\n  }\n}\n</script>\n\n<style>\n.modal-body{\n    display: flex;\n    flex-direction: column;\n}\n\ntextarea{\n    resize: none;\n}\n</style>\n"]}]}